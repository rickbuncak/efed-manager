<h1>Sign up</h1>

<form action="/register" method="POST">
	<label for="username">
		Email:
    	<input type="text" name="username" placeholder="username">
    </label>
    <label for="password">
    	Password:
    	<input type="password" name="password" placeholder="password">
	</label>
    <button>Submit</button>
</form>
